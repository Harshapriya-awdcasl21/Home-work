class Solution {

    public int singleNumber(int[] nums) {

        int t=0;

        int n=nums.length;

        for(int i=0;i<n;i++)

       {int u=0;

         for(int j=0;j<n;j++)

         {if(nums[i]==nums[j])

             u++;

         }

          if(u==1)

              t=nums[i];

         

        }

        return t; 

    }

}
