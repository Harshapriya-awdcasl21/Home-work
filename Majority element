class Solution {

    public int majorityElement(int[] nums) {    

    int t=0;

    int n=nums.length;

   for(int i=0;i<n;i++) {

    int u=0;

    for(int j=0;j<n;j++) {

      if(nums[i]==nums[j]) {

        u++;

      }}

      if ((u>=(n/2)+1)&&(n%2==1)){t=nums[i];

      break;}

      else if((u>=(n/2))&&(n%2==0))

      {t=nums[i];

      break;

      

     }

    }

        return t;

  }

 }
 
 
 
