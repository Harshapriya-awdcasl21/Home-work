class Solution {

    public void reverseString(char[] s) {

     int k=s.length;

     for(int i=0;i<k;i++)

     {

       char temp=s[i];

       s[i]=s[k-1];

       s[k-1]=temp;

       k--;

     }

     for(int i:s)

    System.out.println(i);

   } 

}


